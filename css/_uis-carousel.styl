.uis-carousel {
  position relative

  &_thumbs {
    position: absolute;
    width: 256px;
    bottom: 12px;
    left: 80px;
  }

  &_thumbs&_card-module {
    position static
    text-align center
    width auto
    overflow hidden
  }

  img {
    display block
  }

  &__prev-button {

    &_card-module {
      position absolute
      top 50%
      left 5px
      z-index 2
      margin-top -65px
      cursor pointer
      display inline-block
      width 60px
      height 120px
      background-repeat no-repeat
      background-position center
      background-image url("data:image/svg+xml;base64,")
      border-radius 5px
      border 2px solid transparent
      overflow hidden
      color #fff
      text-indent -1000px

      &:hover {
        border-color currentColor
      }
    }
  }

  &__next-button {

    &_card-module {
      position absolute
      top 50%
      right 5px
      z-index 2
      margin-top -65px
      cursor pointer
      display inline-block
      width 60px
      height 120px
      background-repeat no-repeat
      background-position center
      background-image url("data:image/svg+xml;base64,")
      border-radius 5px
      border 2px solid transparent
      overflow hidden
      color #fff
      text-indent -1000px

      &:hover {
        border-color currentColor
      }
    }

