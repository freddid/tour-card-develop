  }

  &__stage-wrapper {
    position relative
    overflow hidden
    margin 0
    padding 0
  }

  &__stage {
    margin 0
    padding 0
    list-style none
    white-space nowrap
    position relative
    transition left 300ms linear
    left 0
  }

  &__item {
    display inline-block
    vertical-align top
    width 100%

    ^[0]_thumbs &_card-module {
      width auto
      height auto

      /*& div {
        background-size cover
        background-position center
        background-repeat no-repeat
        height 450px
      }*/


    }
/*      & div {
        width 100% !important
      }
    }*/

    &_thumbs img {
      border 2px solid #fff
    }

    &_active img {
      border 2px solid #FFCC00
    }

    .uis-carousel_main & div {
      overflow hidden
      width auto !important
      height auto !important

      & img {
        max-width 100%
      }
    }
  }

  &__dots {
    position absolute
    bottom 20px
    left 0
    right 0
    text-align center
    z-index 1

    &-item {
      display inline-block
      width 10px
      height 10px
      border 1px solid currentColor
      border-radius 50%
      cursor pointer
      margin 0 4px

      &_active {
        background-color currentColor
      }
    }

  }
}